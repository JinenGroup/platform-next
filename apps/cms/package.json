{
  "name": "cms",
  "description": "Payload project created from blank template",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon",
    "build:payload": "payload build",
    "build:server": "tsc",
    "build": "npm run copyfiles && npm run build:payload && npm run build:server",
    "serve": "NODE_ENV=production node dist/server.js",
    "seed": "ts-node src/seed.ts",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "generate:types": "payload generate:types"
  },
  "dependencies": {
    "@payloadcms/plugin-form-builder": "^1.0.15",
    "@payloadcms/plugin-nested-docs": "^1.0.4",
    "@payloadcms/plugin-search": "^1.0.0",
    "@payloadcms/plugin-seo": "^1.0.13",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^10.0.0",
    "express": "^4.17.1",
    "payload": "^1.7.2",
    "payload-plugin-password-protection": "^0.0.2",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/node": "^18.15.11",
    "@types/react": "^18.2.7",
    "copyfiles": "^2.4.1",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.8.4"
  }
}
