{
  "name": "cms",
  "version": "0.0.11-alpha",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "postinstall": "pnpm generate:types",
    "dev": "pnpm build:server && cross-env PAYLOAD_CONFIG_PATH=src/payload-config.ts NODE_ENV=development nodemon",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload-config.ts payload build",
    "build:server": "rm -rf dist/* && tsc",
    "build": "pnpm build:payload && pnpm build:server && pnpm copyfiles",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/ && copyfiles -u 1 \"build/**/*\" ../server/build",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload-config.ts payload generate:types",
    "lint": "prettier --check --ignore-path=../../.prettierignore . && eslint \".\"",
    "format": "prettier --write --ignore-path=../../.prettierignore ."
  },
  "dependencies": {
    "@jinen/helpers": "workspace:^",
    "express": "^4.18.2",
    "payload": "^1.6.9",
    "styled-components": "6.0.0-rc.3"
  },
  "devDependencies": {
    "@jinen/annotations": "workspace:^",
    "@jinen/cms-annotations": "workspace:^",
    "@jinen/eslint-config-custom": "workspace:^",
    "@payloadcms/plugin-form-builder": "^1.0.15",
    "@payloadcms/plugin-nested-docs": "^1.0.4",
    "@payloadcms/plugin-search": "^1.0.0",
    "@payloadcms/plugin-seo": "^1.0.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.27",
    "@types/react-router-dom": "^5.3.3",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.3.1",
    "nodemon": "^2.0.20",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-router-dom": "^5.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "zod": "^3.21.4"
  },
  "peerDependencies": {
    "dotenv": "^16.1.4",
    "react": "^18.2.0",
    "react-router-dom": "^5.3.4"
  }
}
