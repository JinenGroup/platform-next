{
  "name": "cms",
  "description": "Payload project created from blank template",
  "version": "0.0.0",
  "main": "dist/server.js",
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts ts-node-dev --respawn --transpile-only src/server.ts",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "build:server": "tsc",
    "build": "npm run copyfiles && npm run build:payload && npm run build:server",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "seed": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts ts-node --esm src/seed.ts",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types"
  },
  "dependencies": {
    "@jinen/jinen-helpers": "workspace:^",
    "@payloadcms/plugin-form-builder": "^1.0.15",
    "@payloadcms/plugin-nested-docs": "^1.0.4",
    "@payloadcms/plugin-search": "^1.0.0",
    "@payloadcms/plugin-seo": "^1.0.13",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "payload": "^1.7.2",
    "payload-plugin-password-protection": "^0.0.2",
    "react": "^18.2.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/node": "^18.15.11",
    "@types/react": "^18.2.7",
    "copyfiles": "^2.4.1",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^2.0.0"
  }
}
