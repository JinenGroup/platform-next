{
  "name": "@jinen/helpers",
  "version": "0.0.0",
  "main": "src/index.ts",
  "exports": {
    ".": {
      "serve": {
        "types": "./dist/index.js",
        "require": "./dist/index.js",
        "import": "./dist/index.js"
      },
      "default": {
        "types": "./src/index.ts",
        "require": "./src/index.ts",
        "import": "./src/index.ts"
      }
    }
  },
  "scripts": {
    "lint": "prettier --check --ignore-path=../../.prettierignore . && eslint \".\"",
    "format": "prettier --write --ignore-path=../../.prettierignore ."
  },
  "devDependencies": {
    "@jinen/eslint-config-custom": "workspace:*",
    "deepmerge": "^4.3.1"
  }
}
