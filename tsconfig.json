{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "jsx": "react",
    "module": "ESNext",
    "target": "ES2021",
    "sourceMap": true,
    "declaration": true,
    "noEmitOnError": true,
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
    "noEmit": false
  },
  "exclude": ["node_modules", "dist", "build", ".turbo"]
}
