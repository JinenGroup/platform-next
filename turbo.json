{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "cache": false,
      "dependsOn": ["^build"],
      "outputs": [".svelte-kit/**", ".vercel/**", "lib/**", "dist/**", "build/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "serve": {
      "cache": false,
      "dependsOn": ["^serve"]
    }
  },
  "globalEnv": [
    "NODE_ENV",
    "JINEN_SERVER_PORT",
    "JINEN_SERVER_URL",
    "JINEN_CLIENT_URLS",
    "PAYLOAD_SECRET",
    "PAYLOAD_CONFIG_PATH",
    "MONGODB_URI"
  ]
}
